{
  "plugins": [
    "react"
  ],
  "parserOptions": {,
    "sourceType": "module",
    ecmaVersion: 6,
    "ecmaFeatures": {
      "jsx": true,
      "modules":true
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },

  "rules": {
    "no-unused-vars": [1],
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2,
    "react/react-in-jsx-scope": 2,
  }
}
